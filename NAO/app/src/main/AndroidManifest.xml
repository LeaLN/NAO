<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.naotictactoe.nao">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.MancheConfig"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".view.activity.EndGameActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".view.activity.EtatRobot"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".view.activity.GameCurrent"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".view.activity.StatistiquesJeu"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>